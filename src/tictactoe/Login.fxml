<AnchorPane id="AnchorPane" prefHeight="141.0" prefWidth="403.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.LoginController">
   <children>
      <VBox prefHeight="141.0" prefWidth="403.0">
         <children>
            <HBox prefHeight="28.0" prefWidth="361.0">
               <children>
                  <Label prefHeight="26.0" prefWidth="88.0" text="Player 1 Name">
                     <HBox.margin>
                        <Insets left="30.0" right="20.0" top="30.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="Name1" prefHeight="27.0" prefWidth="231.0">
                     <HBox.margin>
                        <Insets left="10.0" top="30.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox prefHeight="0.0" prefWidth="403.0">
               <children>
                  <Label prefHeight="22.0" prefWidth="89.0" text="Player 2 Name">
                     <HBox.margin>
                        <Insets left="30.0" right="30.0" top="20.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="Name2" prefHeight="27.0" prefWidth="231.0">
                     <HBox.margin>
                        <Insets top="20.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
            <HBox prefHeight="5.0" prefWidth="403.0">
               <children>
                  <Button fx:id="mStartButton" mnemonicParsing="false" onAction="#didClickStartButton" prefHeight="27.0" prefWidth="66.0" text="Start ">
                     <HBox.margin>
                        <Insets left="240.0" top="20.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="mCloseButton" mnemonicParsing="false" onAction="#didClickCloseButton" prefHeight="16.0" prefWidth="61.0" text="Cancel">
                     <HBox.margin>
                        <Insets bottom="24.0" left="10.0" top="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
